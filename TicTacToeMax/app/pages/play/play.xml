<Page navigatingTo="navigatingTo" >
	<Repeater items="{{ board }}">
		<Repeater.itemsLayout>
			<GridLayout columns="*, *, *, *, *, *, *, *, *" rows="*, *, *, *, *, *, *, *, *" />
		</Repeater.itemsLayout>
		<Repeater.itemTemplate>
			<Button col="{{ 3*boardCol+cellCol }}" row="{{ 3*boardRow+cellRow }}" text="{{ value }}" tap="makeMove"/>
		</Repeater.itemTemplate>
	</Repeater>
</Page>